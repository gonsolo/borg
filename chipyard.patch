@@ -0,0 +1,92 @@
+#include <stdio.h>
+#include "mmio.h"
+
+#define BORG_TEST1 0x4000
+#define BORG_READ_KICK 0x4020
+#define BORG_WRITE_KICK 0x4040
+#define BORG_COMPLETED 0x4060
+#define BORG_DMA_BASE 0x88000000L
+
+int main(void)
+{
+        uint32_t test1 = reg_read32(BORG_TEST1);
+        printf("Borg test1: %d\n", test1);
+
+        //uint32_t kick = reg_read32(BORG_READ_KICK);
+        //printf("Kick: %d\n", kick);
+        //uint32_t done = reg_read32(BORG_READ_DONE);
+        //printf("Done: %d\n", done);
+
+        uint64_t vals[] = {
+                0x1111222233334444, // 8 bytes, 1 line
+                0x2222333344445555,
+                0x3333444455556666,
+                0x4444555566667777,
+
+                0x5555666677778888,
+                0x6666777788889999,
+                0x777788889999AAAA,
+                0x88889999AAAABBBB, // 64 bytes
+
+                0x9999AAAABBBBCCCC,
+                0xAAAABBBBCCCCDDDD,
+                0xBBBBCCCCDDDDEEEE,
+                0xCCCCDDDDEEEEFFFF,
+
+                0xFFFFFFFFFFFFFFFF,
+                0xFFFFFFFFFFFFFFFF,
+                0xFFFFFFFFFFFFFFFF,
+                0xFFFFFFFFFFFFFFF0, // 128 bytes, 16 lines
+        };
+        int stride = 8; // bytes
+        int lines= 16;
+        puts("Writing memory.\n");
+        for (int i = 0; i < lines ; i++) {
+                reg_write64(BORG_DMA_BASE + i * stride, vals[i]);
+        }
+
+        puts("Kick.\n");
+        reg_write32(BORG_WRITE_KICK, 1);
+        //kick = reg_read32(BORG_READ_KICK);
+        //printf("Kick is: %d\n", kick);
+
+        while (reg_read32(BORG_COMPLETED) != 1) {
+                puts("Waiting for completion.\n");
+        }
+
+        uint64_t vals2[] = {
+                0xA111222233334444, // 8 bytes, 1 line
+                0xA222333344445555,
+                0xA333444455556666,
+                0xA444555566667777,
+
+                0xA555666677778888,
+                0xA666777788889999,
+                0xA77788889999AAAA,
+                0xA8889999AAAABBBB, // 64 bytes
+
+                0xA999AAAABBBBCCCC,
+                0xAAAABBBBCCCCDDDD,
+                0xABBBCCCCDDDDEEEE,
+                0xACCCDDDDEEEEFFFF,
+
+                0xAFFFFFFFFFFFFFFF,
+                0xAFFFFFFFFFFFFFFF,
+                0xAFFFFFFFFFFFFFFF,
+                0xAFFFFFFFFFFFFFF0, // 128 bytes, 16 lines
+        };
+        puts("Writing memory again.\n");
+        for (int i = 0; i < lines ; i++) {
+                reg_write64(BORG_DMA_BASE + i * stride, vals2[i]);
+        }
+
+        puts("Kick.\n");
+        reg_write32(BORG_WRITE_KICK, 1);
+
+        while (reg_read32(BORG_COMPLETED) != 1) {
+                puts("Waiting for completion again.\n");
+        }
+        puts("Returning.\n");
+        return 0;
+}
